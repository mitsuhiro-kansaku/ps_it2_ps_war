var $ajaxs=[];
## クイックリンク件数を取得し、セットします。
function quickLinkCountByAjax(){
    $('td.MenuDetail').each(function(){
        var ids=[];
        $(this).find('span.QuickLinkId').each(function(){
            ids.push(cleanXss($(this).attr('id')));
        });
        if(ids.length==0){return;}
        $ajax=$.ajax({
            type:"POST",
            url:"../quickLinkCountController/getCount",
            data:{ids:ids.toString()},
            success:function(data){
                for(var key in data.map){
                	if ($('span:[id=\"'+key+'\"]').text() == '#customSpringMessage('PAGING_COUNTING')'){
                	    $('span:[id=\"'+key+'\"]').text(data.map[key]);
                	}
                }
            },
            error:function(xhr,textStatus,errorThrown){
                if(textStatus=="timeout"){
                    top.location.href="$!requestContext.contextPath/layout/sessiontimeout"
                }
            }
        });
        $ajaxs.push($ajax);
    });
};
## アンロードされる直前に発生します。
window.addEventListener('beforeunload',function(e){
    $.each($ajaxs,function(index,value){
        value.abort();## キャンセルする
    });
},false);
