    <td $!_td_style_left align="right">
        #customSpringMessage($!itemDefinitionDetail.elementNameCd)
    </td>
    <td style="padding:0;"><table class="NoBorder" width="100%">
        <input id="$!property" name="$!property" type="hidden" value="$!propertyValue">
        <tr class="FontBoxHeader">
            <td class="Box1H" style="border-left: 0; border-top: 0;" colspan="2">##課税区分
                #customSpringMessage('TAX_IND')
            </td>
            <td class="Box1H" style="border-top: 0;">##請求金額（税込み）
                #customSpringMessage('CLAIM_AMOUNT_IN_TAX')
            </td>
            <td class="Box1H" style="border-right: 0; border-top: 0;">##消費税額
                #customSpringMessage('CONSUMPT_AMOUNT')
            </td>
        </tr>
        <tr class="FontSmall">
            <td class="Box1D" style="border-left: 0;" width="24%" height="24px">##課税取引
                $!_taxCdTaxName
            </td>
            <td class="Box1D" width="10%">##税率
                <span $!_tax_rate_style id="$!taxRate" name="$!taxRate">$!taxRateValue</span>&nbsp;%
            </td>
            <td class="Box1D" width="33%">##請求金額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!taxClaimAmount" name="$!taxClaimAmount">$!taxClaimAmountValue</span>
            </td>
            <td class="Box1D" style="border-right: 0;" width="33%">##消費税額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!taxConsumptAmount" name="$!taxConsumptAmount">$!taxConsumptAmountValue</span>
            </td>
        </tr>
        <tr class="FontSmall">
            <td class="Box1D" style="border-left: 0;" height="24px">##軽減税率対象
                $!_taxCdReducedTaxName
            </td>
            <td class="Box1D">##税率
                <span $!_tax_rate_style id="$!reducedTaxRate" name="$!reducedTaxRate">$!reducedTaxRateValue</span>&nbsp;%
            </td>
            <td class="Box1D">##請求金額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!reducedTaxClaimAmount" name="$!reducedTaxClaimAmount">$!reducedTaxClaimAmountValue</span>
            </td>
            <td class="Box1D" style="border-right: 0;">##消費税額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!reducedTaxConsumptAmount" name="$!reducedTaxConsumptAmount">$!reducedTaxConsumptAmountValue</span>
            </td>
        </tr>
        <tr class="FontSmall">
            <td class="Box1D" style="border-left: 0;" height="24px" colspan="2">##非課税取引
                $!_taxCdNonTaxName
            </td>
            <td class="Box1D">##請求金額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!nonTaxClaimAmount" name="$!nonTaxClaimAmount">$!nonTaxClaimAmountValue</span>
            </td>
            <td class="Box1D" style="border-right: 0;">##消費税額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!nonTaxConsumptAmount" name="$!nonTaxConsumptAmount">$!nonTaxConsumptAmountValue</span>
            </td>
        </tr>
        <tr class="FontSmall">
            <td class="Box1D" style="border-left: 0;" height="24px" colspan="2">##免税取引
                $!_taxCdTaxExemptionName
            </td>
            <td class="Box1D">##請求金額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!taxExemptionClaimAmount" name="!taxExemptionClaimAmount">$!taxExemptionClaimAmountValue</span>
            </td>
            <td class="Box1D" style="border-right: 0;">##消費税額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!taxExemptionConsumptAmount" name="$!taxExemptionConsumptAmount">$!taxExemptionConsumptAmountValue</span>
            </td>
        </tr>
        <tr class="FontSmall">
            <td class="Box1D" style="border-left: 0;" height="24px" colspan="2">##消費税対象外取引
                $!_taxCdUnTaxName
            </td>
            <td class="Box1D">##請求金額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!unTaxClaimAmount" name="$!unTaxClaimAmount">$!unTaxClaimAmountValue</span>
            </td>
            <td class="Box1D" style="border-right: 0;">##消費税額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!unTaxConsumptAmount" name="$!unTaxConsumptAmount">$!unTaxConsumptAmountValue</span>
            </td>
        </tr>
        <tr class="FontSmall">
            <td class="Box1D" style="border-left: 0; border-bottom: 0;" height="24px" colspan="2">##合計
                #customSpringMessage('CLAIM_TOTAL')
            </td>
            <td class="Box1D" style="border-bottom: 0;">##請求金額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!totalClaimAmount" name="$!totalClaimAmount">$!totalClaimAmountValue</span>
            </td>
            <td class="Box1D" style="border-right: 0; border-bottom: 0;">##消費税額
                <span $!_currency_mark_style>$!currencyMarkValue</span>
                <span $!_amount_style id="$!totalConsumptAmount" name="$!totalConsumptAmount">$!totalConsumptAmountValue</span>
            </td>
        </tr>
    </table></td>