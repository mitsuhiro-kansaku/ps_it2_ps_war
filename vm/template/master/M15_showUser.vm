    <td $!_td_style_left nowrap>
        #customSpringMessage($!itemDefinitionDetail.elementNameCd)
    </td>
    <td $!_td_style_right nowrap>
        <table class="NoBorder">
            <tr>
                <td valign="top" rowspan="2">
                     <img class="Mark" src="../../image/mrkHuman.gif">
                </td>
                <td nowrap>
                     <span class="FontSmall">
                        $!sectionCdValue
                        $!sectionNameValue
                     </span>
                </td>
                <td>
                     <img class="Mark" src="../../image/mrkTel.gif">
                </td>
                <td class="FontMini" nowrap>#customSpringMessage('EXTENSION_NO'):
                     <span class="FontSmallAnk">
                        $!extensionNoValue
                     </span>&nbsp;&nbsp;#customSpringMessage('EXTERNAL_NO'):
                     <span class="FontSmallAnk">
                        $!telNoValue
                     </span>
                </td>
           </tr>
           <tr>
                <td>
                    <span class="FontSmall">
                        $!userCdValue
                     </span>
                     <span class="FontUserName">
                        $!userNameValue
                     </span>
                </td>
                <td>
                     <img class="Mark" src="../../image/mrkMail.gif">
                </td>
                <td nowrap>
                     <span class="FontMiniAnk">
                        <a href='mailto:$!userNameValue<$!emailAddrValue>'>$!emailAddrValue</a>
                     </span>
                </td>
                <td align="right" valign="bottom">
                </td>
           </tr>
       </table>
    </td>

