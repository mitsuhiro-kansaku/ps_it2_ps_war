                                                                                #springFormHiddenInput("domain.receive.appManagement.appList.appUserName.official.officialName" "")
                                                                                #springFormHiddenInput("domain.receive.appManagement.appList.appUserName.section.sectionPlace.sectionPlaceCd" "")
                                                                                #springFormHiddenInput("domain.receive.appManagement.appList.appUserName.section.sectionPlace.sectionPlaceName" "")
                                                                                #springFormHiddenInput("domain.receive.appManagement.appList.appUserName.mailAddr.mail" "")
