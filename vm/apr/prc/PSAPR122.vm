#parse("./common/common_js.vm")
var isClickable = false;

function onLoad() {
    #set($commonTitle = "#customSpringMessage('PRICE_M_REGISTER_APP')")

    var alertMessage = "$!alertMessage";
    if (alertMessage != "") {
        alert(alertMessage);
        if(alertMessage.indexOf("#customSpringMessage('E035')") != -1){
            buttonDisable();
            parent.iframeChange("${flowExecutionUrl}&_eventId=next");
            return;
        }
    }

    approvalPaging();

    var errorMsg = "$!errorMessage";
    if (errorMsg != "") {
        document.getElementById("TR_ERROR").style.display = "";
        isClickable = true;
        return;
    }

    var isReturnSuccess = '#jsEscape($!isReturnSuccess)';
    if(isReturnSuccess == 'true') {
        alert('#customMessageArgs("I029", ["#customSpringMessage('PRICE_M_REGISTER')"])');
        buttonDisable();
        parent.iframeChange("${flowExecutionUrl}&_eventId=next");
        return;
    }

    var isApprovalSuccess = '#jsEscape($!isApprovalSuccess)';
    if(isApprovalSuccess == 'true') {
        alert('#customMessageArgs("I028", ["#customSpringMessage('PRICE_M_REGISTER')"])');
        buttonDisable();
        parent.iframeChange("${flowExecutionUrl}&_eventId=next");
        return;
    }

    //approvalPaging();
    isClickable = true;
}

//差戻し
function sendBack() {
    if(!isClickable) {
        return;
    }
    #set( $priceMRegisterAppMsg = "#customSpringMessage('PRICE_M_REGISTER')" )
    #set( $actionRegisterMsg = "#customSpringMessage('RETURN')" )
    if( !confirm('#customMessageArgs("W001", [$priceMRegisterAppMsg, $actionRegisterMsg])') ) {
        return;
