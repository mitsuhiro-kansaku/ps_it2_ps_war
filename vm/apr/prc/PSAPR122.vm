    ## +++++ 強化-00041-000 2013/03/25 ADD SBO_KANGJW -END- +++++
    ## +++++ IT2-01209-000 2013/03/18 ADD SBO_KANGJW START +++++
    getObj("Body").action = "${flowExecutionUrl}&_eventId=sendBack";
    ## +++++ IT2-01209-000 2013/03/18 ADD SBO_KANGJW END +++++
    getObj("Body").submit();
}

//承認
