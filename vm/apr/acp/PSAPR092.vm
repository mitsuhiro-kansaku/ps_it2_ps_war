    if(!isClickable) {
        return;
    }
    getObj("Body").action="${flowExecutionUrl}&_eventId=addAuthorizer";
    getObj("Body").submit();
}
function commentPassed(){
    var comment = getObj("accept.appManagement.backApp.backAppReason.backReason.comment");
    if(comment.value.length>200){
       alert("差戻理由を入力してください。（200バイト以内）");
       return false;
