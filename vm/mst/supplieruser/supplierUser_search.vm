var newPageId="supplierUserRegist";

#parse("./common/common_js.vm")
function onLoad(){
    top.document.title = "#customSpringMessage('M_S_USER')"; 
    //bodyLoad();
    var alertMessage = "$!alertMessage";
    if (alertMessage != ""){
        alert(alertMessage);
    }
    var errorMsg = "$!errorMessage";
    if (errorMsg != ""){
        document.getElementById("TR_ERROR").style.display = "";
        return;
    }
    var isShowList = "#jsEscape($isShowList)";
    if (isShowList == 'true'){
        iframeChange("${flowExecutionUrl}&_eventId=search","pageid","supplierUserList");
    }else {
         execSearch();
    }
}

function execSearch() {
    getObj("BodyForm").action = "${flowExecutionUrl}&_eventId=validate";
    getObj('BodyForm').submit();
}

