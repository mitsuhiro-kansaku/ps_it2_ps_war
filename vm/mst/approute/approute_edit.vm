        if( !confirm('#customMessageArgs("W001",[$mmApproute,$register])') ) {
            return;
        }
    #end
    
    buttonDisable();
    getObj("Body").submit();
}
function getCalendar(num,objName){
    var obj = document.getElementsByName(objName)[0];
    initSearchBox(num,obj);
}

$(function () {
    var companyCd= '#jsEscape($!domain.approuteIdentify.companyCd)';
    var voucherInd='#jsEscape($!domain.approuteIdentify.approuteVoucherInd.approuteVoucherInd)';
    var authNo;
    if(voucherInd=='0'){
        //カタログ購入依頼
        authNo = '530';
    }else if(voucherInd=='1'){
        //都度購入依頼
        authNo = '530,540';
    }else if(voucherInd=='2'){
        //注文
        authNo = '570';
    }else if(voucherInd=='3'){
        //受入
        authNo = '580';
    }else if(voucherInd=='4'){
        //検査
        authNo = '590';
    }else if(voucherInd=='5'){
        //検収
        authNo = '600';
    }else if(voucherInd=='7'){
        //支払請求
        authNo = '550';
    }else if(voucherInd=='8'){
        //支払伝票
        authNo = '670';
    }else if(voucherInd=='A'){
        //見積取得依頼
        authNo = '520';
    }else if(voucherInd=='B'){
        //見積依頼
        authNo = '560';
    }else if(voucherInd=='C'){
        //見積回答受付
        authNo = '560';
    }else if(voucherInd=='D'){
        //購買価格
        authNo = '620';
    }else if(voucherInd=='E'){
        //価格改定
        authNo = '620';
    }else if(voucherInd=='F'){
        //回答価格改定
        authNo = '620';
    }else if(voucherInd=='G'){
        //価格一括登録
        authNo = '620';
    }
    for(var i = 1; i <= 9; i++) {
        var imgButtonObj = getObj("UserSearch" + i);
        userPopupFun(imgButtonObj,companyCd,authNo,i);
    }
});

function userPopupFun(imgButtonObj,companyCd,authNo,index) {
    imgButtonObj.onclick = function() {
        getObj("usernum").value = index;
        userPopup.setParam("0",companyCd,"","","1",authNo,"","1");
        userPopup.setObject(
            getObj("approuteSortItemList.ApprouteSortItem" + index + ".approuterInfo.userId"),
            getObj("approuteSortItemList.ApprouteSortItem" + index + ".approuterInfo.userName"),
            getObj("approuteSortItemList.ApprouteSortItem" + index + ".approuterInfo.sectionCd"),
            getObj("approuteSortItemList.ApprouteSortItem" + index + ".approuterInfo.sectionName"),
            getObj("approuteSortItemList.ApprouteSortItem" + index + ".approuterInfo.official.officialName"),
            null,null,null,null,null,null,
            getObj("sectionOfficialName" + index)
        );
        masterSearch("../../src/userPopup/search?pageid=userPopupSearch&popupPageid=PSMST182","userSearch","700","500");
        getObj("approuteSortItemList.ApprouteSortItem" + index + ".approuterInfo.userId").focus();
   };
}

function clearInfo(index) {
    var inputs = getObj("Adjust" + index).getElementsByTagName("input");
    for (var i = 0; i < inputs.length; i++) {
       //各テキスト ボックスの空
       if (inputs[i].type == "text") {
           inputs[i].value ="";
       }
       if (inputs[i].type == "hidden") {
           inputs[i].value ="";
       }
    }
    getObj("sectionOfficialName" + index).innerText = "";
    var obj = document.getElementsByName("Cancel"+index)[0];
    obj.style.display="none"
}

function formateMessage() {
    var alertStr= document.getElementById('textAlert').value;
    document.getElementById('textAlert').value = alertStr.replace(/\r\n/g,"");
    var errorStr= document.getElementById('textError').value;
    document.getElementById('textError').value = errorStr.replace(/\r\n/g,"");
}
