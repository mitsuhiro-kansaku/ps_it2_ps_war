                alert('#customMessageArgs("I005",[$mmApproute])');
            #end
            
            buttonDisable();
            top.iframeChange("${flowExecutionUrl}&_eventId=back");
        }
    
}

function changeCancel(userId,index){
    var obj = document.getElementsByName("Cancel"+index)[0];
    if(userId.value != ""){
        obj.style.display="inline";
    }
    if(userId.value == ""){
        obj.style.display="none"
    }
}

function exec() {
    #set( $mmApproute = "#customSpringMessage('M_APP_ROUTE')" )   
    #if($!type == "Update")
        #set( $register = "#customSpringMessage('UPDATE')" )
        if( !confirm('#customMessageArgs("W001",[$mmApproute,$register])') ) {
