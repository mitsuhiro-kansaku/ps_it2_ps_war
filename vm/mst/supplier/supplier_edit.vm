        if( !confirm('#customMessageArgs("W001",[$mmSupplier,$register])') ) {
            return;
        }
    #end
    
    buttonDisable();
    getObj("Body").submit();
}

function getCalendar(num,objName){
    var obj = document.getElementsByName(objName)[0];
