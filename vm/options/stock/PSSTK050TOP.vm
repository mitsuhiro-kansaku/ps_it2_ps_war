<body  class="DetailBody" onload="onLoad();" style="overflow-y:hidden;">
<form name="Body"  >
    <table class="DetailSheet" style="width:98%; height:100%;" align="left">
        <tr>
            <td class="NoBorder" style="width:100%; height:0px;">
                <table class="NoBorder" style="width:100%; ">
                    <tr>
                        <td class="DTL_MSGBOX" align="right">
                            <input id="closeButton" type="button" class="MiddleButton" value=#customSpringMessage('CLOSE') onClick="window.close();">
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td class="NoBorder" style="width:100%; height:0px;">
                <table class="NoBorder" width="100%">
                    <tr>
                        <td id="TabHeader_0" class="DetailTabsOn" value="" nowrap>
                            <a class="Normal" href="javascript:changeTab();">#customSpringMessage('STRG_RESULTS_DTL')</a>
                        </td>
                        <td class="DetailTabsNone">&nbsp;</td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td  class="InterTab">
                <iframe id="DATAAREA" name="DATAAREA" framespacing="0" frameborder="0" src="" scrolling="yes" frameborder="no"
                style="position:static;width:100%;border:0px solid #999999;height:100%;z-index:0;"></iframe>
            </td>
        </tr>
        <tr>
            <td class="NoBorder" style="width:100%; height:0px;">
                <table class="NoBorder" style="width:100%; ">
