<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
    parent="baseSearchInsert">

    <view-state id="subjectSearch" view="mst/subject/subject_search"
        parent="baseSearchInsert#search">
        <transition on="back" to="home" validate="false"/>
    </view-state>

    <subflow-state id="list" subflow="mst/subject/list" parent="baseSearchInsert#list">
        <transition on="back" to="subjectSearch" >
        	<set name="flowScope.isCurrentPageSearch" value="true" />
        </transition>
    </subflow-state>
    
    <subflow-state id="insert" subflow="mst/subject/insert" parent="baseSearchInsert#insert">
        <transition on="back" to="subjectSearch"/>
    </subflow-state>

</flow>